{
  "name": "jamai",
  "version": "0.1.5",
  "description": "JamAIBase Client SDK (JS/TS)",
  "main": "./index.js",
  "module": "./index.mjs",
  "umd:main": "./index.umd.js",
  "unpkg": "./index.umd.js",
  "types": "./index.d.ts",
  "scripts": {
    "test": "jest",
    "clear-cache": "jest --clearCache",
    "build": "/bin/bash build",
    "mock-server": "prism mock openapi.json --port 12233",
    "knowledge-examples": "node examples/knowledge_table.js",
    "action-examples": "node examples/action_table.js",
    "chat-examples": "node examples/chat_table.js",
    "llm-examples": "node examples/chat_completion.js",
    "openapi-to-zod": "openapi-zod-client openapi.json -o zodschema/zodmodels.ts",
    "doc-ts": "typedoc --tsconfig tsconfig.build.json ./dist/**/*.ts --exclude '**/*.d.ts' --out docs-autogen-ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/EmbeddedLLM/jamaisdk.git"
  },
  "private": false,
  "keywords": [
    "jam-ai",
    "jamai"
  ],
  "imports": {
    "jamai": "./",
    "jamai/*": "./src/*"
  },
  "exports": {
    ".": {
      "browser": {
        "require": {
          "types": "./index.d.ts",
          "default": "./index.js"
        },
        "import": {
          "types": "./index.d.ts",
          "default": "./index.mjs"
        },
        "types": "./index.d.ts",
        "default": "./index.umd.js"
      },
      "node": {
        "require": {
          "types": "./index.d.ts",
          "default": "./index.js"
        },
        "import": {
          "types": "./index.d.ts",
          "default": "./index.mjs"
        },
        "types": "./index.d.ts",
        "default": "./index.umd.js"
      },
      "default": "./index.umd.js",
      "types": "./index.d.ts"
    },
    "./*": {
      "types": "./*.d.ts",
      "require": "./*.js",
      "default": "./*.mjs"
    },
    "./index.mjs": {
      "types": "./index.d.ts",
      "default": "./index.mjs"
    },
    "./index.js": {
      "types": "./index.d.ts",
      "default": "./index.js"
    },
    "./index.umd.js": {
      "types": "./index.d.ts",
      "default": "./index.umd.js"
    }
  },
  "author": "EmbeddedLLM, Tan Tun Jian",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/EmbeddedLLM/jamaisdk/issues"
  },
  "homepage": "https://github.com/EmbeddedLLM/jamaisdk#readme",
  "dependencies": {
    "tsc-alias": "^1.8.8",
    "@typescript-eslint/parser": "^7.7.0",
    "agentkeepalive": "^4.5.0",
    "axios": "^1.6.8",
    "axios-retry": "^4.1.0",
    "eventsource": "^2.0.2",
    "expect.js": "^0.3.1",
    "zod": "^3.22.5",
    "zod-form-data": "^2.0.2"
  }
}
