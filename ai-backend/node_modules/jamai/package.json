{
    "name": "jamai",
    "version": "0.1.6",
    "description": "JamAIBase Client SDK (JS/TS)",
    "main": "dist/index.js",
    "module": "dist/index.mjs",
    "umd:main": "dist/index.umd.js",
    "unpkg": "dist/index.umd.js",
    "types": "dist/index.d.ts",
    "scripts": {
        "test": "jest",
        "clear-cache": "jest --clearCache",
        "build": "/bin/bash build",
        "mock-server": "prism mock openapi.json --port 12233",
        "knowledge-examples": "node examples/knowledge_table.js",
        "action-examples": "node examples/action_table.js",
        "chat-examples": "node examples/chat_table.js",
        "llm-examples": "node examples/chat_completion.js",
        "openapi-to-zod": "openapi-zod-client openapi.json -o zodschema/zodmodels.ts",
        "doc-ts": "typedoc --tsconfig tsconfig.build.json ./dist/**/*.ts --exclude '**/*.d.ts' --out docs-autogen-ts"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/EmbeddedLLM/jamaisdk.git"
    },
    "private": false,
    "keywords": [
        "jam-ai",
        "jamai"
    ],
    "imports": {
        "jamai": "./",
        "jamai/*": "./src/*"
    },
    "exports": {
        ".": {
            "browser": {
                "require": {
                    "types": "./dist/index.d.ts",
                    "default": "./dist/index.js"
                },
                "import": {
                    "types": "./dist/index.d.ts",
                    "default": "./dist/index.mjs"
                },
                "types": "./dist/index.d.ts",
                "default": "./dist/index.umd.js"
            },
            "node": {
                "require": {
                    "types": "./dist/index.d.ts",
                    "default": "./dist/index.js"
                },
                "import": {
                    "types": "./dist/index.d.ts",
                    "default": "./dist/index.mjs"
                },
                "types": "./dist/index.d.ts",
                "default": "./dist/index.umd.js"
            },
            "default": "./dist/index.umd.js",
            "types": "./dist/index.d.ts"
        },
        "./*": {
            "types": "./dist/*.d.ts",
            "require": "./dist/*.js",
            "default": "./dist/*.mjs"
        },
        "./index.mjs": {
            "types": "./dist/index.d.ts",
            "default": "./dist/index.mjs"
        },
        "./index.js": {
            "types": "./dist/index.d.ts",
            "default": "./dist/index.js"
        },
        "./index.umd.js": {
            "types": "./dist/index.d.ts",
            "default": "./dist/index.umd.js"
        }
    },
    "author": "EmbeddedLLM, Tan Tun Jian",
    "license": "Apache-2.0",
    "bugs": {
        "url": "https://github.com/EmbeddedLLM/jamaisdk/issues"
    },
    "homepage": "https://github.com/EmbeddedLLM/jamaisdk#readme",
    "dependencies": {
        "tsc-alias": "^1.8.8",
        "@typescript-eslint/parser": "^7.7.0",
        "agentkeepalive": "^4.5.0",
        "axios": "^1.6.8",
        "axios-retry": "^4.1.0",
        "eventsource": "^2.0.2",
        "expect.js": "^0.3.1",
        "zod": "^3.22.5",
        "zod-form-data": "^2.0.2"
    },
    "devDependencies": {
        "@hey-api/openapi-ts": "^0.40.0",
        "@jest/globals": "^29.7.0",
        "@types/jest": "^29.5.12",
        "@types/node": "^20.12.7",
        "jest": "^29.7.0",
        "jsdoc": "^4.0.2",
        "microbundle": "^0.15.1",
        "openapi-typescript": "^6.7.5",
        "openapi-zod-client": "^1.18.1",
        "ts-jest": "^29.1.2",
        "tsconfig-paths-jest": "^0.0.1",
        "typedoc": "^0.25.13",
        "typescript": "^5.4.5",
        "zod": "^3.22.5"
    }
}
